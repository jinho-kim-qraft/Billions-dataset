   "source": [
    "import pandas as pd\n",
    "\n",
    "def resample_daily_returns(returns: pd.Series, freq: str) -> pd.Series:\n",
    "    return returns.resample(freq).agg(lambda x: (x + 1).prod() - 1)\n",
    "    "
